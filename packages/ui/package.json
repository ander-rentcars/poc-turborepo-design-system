{
  "name": "@acme/ui",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    "./components/*": {
      "types": "./src/components/*.tsx",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    },
    "./globals.css": "./dist/globals.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "postcss": "^8.5.1",
    "react": "^18.2.0",
    "tailwindcss": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
